{
  "name": "fitapp-backend",
  "version": "1.0.0",
  "description": "Backend con TypeScript, Express, PostgreSQL, Resend, Supabase y Clerk",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:check": "node scripts/check-db.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:seed": "tsx src/database/seed.ts",
    "db:docker": "docker-compose up -d postgres",
    "db:docker-stop": "docker-compose down",
    "db:docker-logs": "docker-compose logs postgres",
    "db:admin": "docker-compose up -d adminer",
    "prestart": "npm run db:check",
    "predev": "npm run db:check",
    "setup": "npm run db:generate && npm run db:migrate && npm run build",
    "quick-start": "node scripts/quick-start.js",
    "seed": "node scripts/insert-example-data.js"
  },
  "keywords": [
    "typescript",
    "express",
    "postgresql",
    "resend",
    "supabase",
    "clerk"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.0.3",
    "prisma": "^5.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@supabase/supabase-js": "^2.39.3",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "resend": "^3.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.22.4"
  }
}
